<Project>

  <Target Name="DeleteCKStObjAutoAssemblyFiles" AfterTargets="CoreBuild">
    <ItemGroup>
      <FilesToDelete Include="$(OutDir)CK.StObj.AutoAssembly.dll"/>
      <FilesToDelete Include="$(OutDir)CK.StObj.AutoAssembly.*.dll"/>
    </ItemGroup>
    <Delete Files="@(FilesToDelete)" >
      <Output TaskParameter="DeletedFiles" ItemName="DeletedList"/>
    </Delete>
    <Message Importance="high" Text="Deleted generated files: '@(DeletedList)'"/>
  </Target>

</Project>
